 .syntax unified
.global armSquareRoot
.section .text.rodata


	//extern void armSquareRoot(float *input, float *output);
	// R0 = input pointer
	// R1 = output pointer

armSquareRoot:
	VLDR.f32	S0, [R0] 		// S0 = input
	VSQRT.f32		S0, S0
	VSTR.f32 	S1, [R1]
	BX LR
